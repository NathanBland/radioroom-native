{
  "name": "radioroom",
  "version": "0.0.1",
  "description": "Radio room native client ",
  "main": "index.html",
  "window": {
    "title": "RadioRoom",
    "toolbar": false,
    "frame": true,
    "resizable": true,
    "position": "center",
    "width": 800,
    "height": 800
  },
  "repository": {
    "url": "https://github.com/NathanBland/radioroom-native"
  },
  "scripts": {
    "run": "nwbuild -v 0.12.2 --run ./",
    "win": "nwbuild -v 0.12.2 -p win ./",
    "linux": "nwbuild -v 0.12.2 -p linux32,linux64 ./",
    "osx": "nwbuild -v 0.12.2 -p osx ./"
  },
  "author": {
    "name": "Nathan Bland",
    "url": "nathan.bland@gmail.com"
  },
  "dependencies": {
    "node-webkit-builder": "*",
    "socket.io": "^1.4.4"
  },
  "devDependencies": {
    "gulp": "^3.6.2",
    "gulp-shell": "^0.2.5"
  }
}
